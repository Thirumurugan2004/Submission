#!javascript

function* flatten(array){
    for(let x of array){
        if(x instanceof Array){
            yield* flatten(x);
        }
        else{
            yield x;
        }
    }
}

let test = flatten[1, [2, 3], 4, [5, 6, [7, 8]]];

