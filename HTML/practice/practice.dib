#!javascript

function gen(n){
    return{
        [Symbol.iterator](){
            let i = 0;
            return{
                next(){ 
                    return {
                       done: (i > n) ? true : false,
                       value: i++
                    };
                
                }
            };
        }
    };
    
    
}

for(let v of gen(10)){
    console.log(v);
}

console.log(Array.from(gen(10), x => x * x));

